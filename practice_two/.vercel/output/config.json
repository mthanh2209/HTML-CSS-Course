{
	"version": 3,
	"routes": [
		{
			"src": "^/[^./]+\\.[0-9a-f]{8}\\.(css|js|png|jpg|webp|avif|svg)$",
			"headers": {
				"cache-control": "s-maxage=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "error"
		},
		{
			"status": 404,
			"src": "^(?!/api).*$",
			"dest": "/404.html"
		}
	],
	"framework": {
		"version": "2.9.3"
	},
	"crons": []
}
